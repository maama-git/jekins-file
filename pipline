pipeline
{
agent any
 environment
 {
 NAME="naama"
 }
    
    options{
timeout(time:1,unit:'MINUTES')}



stages

        {
            stage("stage number one")
                 {
                steps{
                    echo "Avital & Rachely"
                    echo"{$NAME}"
                    echo  "build number:${env.BUILD_NUMBER}"
                }

          }  

       


        stage("stage number two")
         {
             steps{
                  echo "hi evryone"
                   }
                            
         }
}
